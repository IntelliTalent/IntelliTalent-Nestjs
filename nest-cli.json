{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gatway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gatway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gatway",
  "projects": {
    "gatway": {
      "type": "application",
      "root": "apps/gatway",
      "entryFile": "main",
      "sourceRoot": "apps/gatway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gatway/tsconfig.app.json"
      }
    },
    "auth": {
